@model IEnumerable<VideoAccessReport>


@*<div class="" id="chart_@(ViewBag.catid)"></div>*@
<table class="table table-bordered">
    <tbody>
        @foreach (var videoReport in Model)
        {
            <tr>
                <td>@videoReport.IPViewCount</td>
                <td><a href="/VideoAccess/VideoReport?videoId=@videoReport.Id" title="Click vào để coi báo cáo chi tiết về video này">@videoReport.Title</a></td>
            </tr>
        }
    </tbody>
</table>


