@model VideoTranscriptModel

<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Đóng</span></button>
        <h4 class="modal-title" id="addModalLabel">Thêm nội dụng</h4>
    </div>
    <div class="modal-body">
        @using (Ajax.BeginForm("Create", "VideoTranscript", null, new AjaxOptions
        {
            InsertionMode = InsertionMode.Replace,
            OnSuccess = "onSucceed",
            UpdateTargetId = "list_transcipt"
        }, new { role = "form" }))
        {
            @Html.Partial("_AddTranscript", Model)
        }
    </div>
</div>