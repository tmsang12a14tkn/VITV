@{
    Layout = "../Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="main-box">
            <header class="main-box-header clearfix">
                <h2>Xóa tập tin tạm</h2>
            </header>
            <div class="main-box-body clearfix">
                @using (Html.BeginForm("DeleteTemp", "Admin", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                {
                    <div class="form-group">
                        <div class="col-lg-offset-2 col-lg-10">
                            <button type="submit" class="btn btn-danger">Xóa</button>
                            @Html.ActionLink("Hủy", "Index", "Admin", new { @class = "btn btn-default" })
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>